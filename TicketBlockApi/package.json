{
  "name": "ticketblock-api",
  "version": "0.0.1",
  "description": "TicketBlock API",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && cd dist && cross-env NODE_ENV=PRODUCTION node -r dotenv/config Index.js",
    "start-windows": "npm run build-windows && cd dist && set NODE_ENV=PRODUCTION && node -r dotenv/config Index.js",
    "dev": "set DEBUG=express:* & nodemon",
    "test": "cross-env NODE_ENV=test mocha test/**/*.ts",
    "build": "npm run clean && tsc --outDir dist && cp .env ./dist && cp -r security ./dist  ",
    "build-windows": "npm run clean && tsc --outDir dist && copy .env dist && mkdir dist\\security && xcopy  security \"dist/security\" /S /Y /i   ",
    "clean": "rimraf dist"
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "@Layinka",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/core-js": "^2.5.4",
    "@types/mime": "^2.0.3",
    "@types/node-fetch": "^2.5.8",
    "@zeit/fetch": "^6.0.0",
    "arweave": "^1.10.15",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "buffer": "^6.0.3",
    "class-validator": "^0.10.0",
    "cluster": "^0.7.7",
    "colors": "^1.4.0",
    "connect-timeout": "^1.9.0",
    "core-js": "^3.12.1",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "ethers": "^5.5.4",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-handlebars-sections": "^1.0.1",
    "express-jwt": "^5.3.1",
    "express-unless": "^2.1.3",
    "find-config": "^1.0.0",
    "hbs": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31",
    "method-override": "^3.0.0",
    "mime": "^2.4.5",
